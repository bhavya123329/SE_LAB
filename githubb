$git config --global user.name "Bhavya"
$git config --global user.email "pk.bhavya2007@gmail.com
$git config --global init.defaultBranch main
$git clone <repo>
$cd <repo>
echo "# demo1" >> file1
git init
git add file1
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/bhavya123329/demo1.git
git push -u origin main
chk repo for file1
create file2 in repo in github
git pull origin main
now chk local for file2 
